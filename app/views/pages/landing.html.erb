<div class="jumbotron landing-header">
  <br /><br /><br />
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <h1 class="text-center hidden-xs hidden-sm">$4 Delivery, On-Demand</h1>
  <h2 class="text-center hidden-md hidden-lg">$4 Delivery, On-Demand</h2>
  <h3 class="text-center hidden-xs hidden-sm">30 Minutes or Less.</h3>
  <h4 class="text-center hidden-md hidden-lg">30 Minutes or Less.</h4>
  <br />
  <div class="col-xs-12 col-sm-8 col-sm-offset-2 col-lg-4 col-lg-offset-4">
    <%= form_tag '', method: :post, id: 'store-location-form' do %>
      <div class="input-group">
        <%= hidden_field_tag :latitude %>
        <%= hidden_field_tag :longitude %>
        <%= search_field_tag :address, nil, class: "form-control landing-address", id: "gmaps-autocomplete", autofocus: true, placeholder: "Enter your address", required: true %>
        <span class="input-group-btn">
          <%= button_tag value: "search", class: "btn btn-default landing-address landing-button white" do %>
            <i class="fa fa-chevron-right"></i>
          <% end %>
        </span>
      </div><!-- /input-group -->
    <% end %>
  </div><!-- /.col-lg-6 -->
  <div class="clearfix"></div>
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <br class="hidden-xs" />
  <br /><br /><br />
  <br /><br /><br />
  <br class="hidden-xs" />
</div>
<div class="jumbotron" style="background:#fff;">
